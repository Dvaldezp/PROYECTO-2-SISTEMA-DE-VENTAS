<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="700.0" prefWidth="900.0" style="-fx-background-color: #992936;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControladorVerProducto">
   <children>
      <Label layoutX="109.0" layoutY="131.0" prefHeight="53.0" prefWidth="136.0" text="Ver Productos">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Label>
      <Button fx:id="verTodoLosProductos" layoutX="376.0" layoutY="184.0" mnemonicParsing="false" onAction="#verTodoLosProductos" prefHeight="32.0" prefWidth="170.0" text="Ver todos los productos" textAlignment="CENTER" textFill="#1e1943">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <TableView fx:id="tblCliente" layoutX="100.0" layoutY="251.0" prefHeight="318.0" prefWidth="700.0">
        <columns>
          <TableColumn fx:id="tfCodigo" prefWidth="75.0" text="Id Producto" />
          <TableColumn fx:id="tfNombre" prefWidth="316.0" text="Nombre" />
            <TableColumn fx:id="tfCantidad" prefWidth="218.0" text="Stock en Inventario" />
            <TableColumn fx:id="tfPrecio" prefWidth="89.0" text="Precio" />
        </columns>
      </TableView>
   </children>
</Pane>
