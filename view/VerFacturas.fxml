<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="700.0" prefWidth="900.0" style="-fx-background-color: #992936;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControladorFacturas">
   <children>
      <Label layoutX="220.0" layoutY="150.0" prefHeight="38.0" prefWidth="227.0" text="Facturas Especificas">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <Button fx:id="verfacturas" layoutX="517.0" layoutY="156.0" mnemonicParsing="false" onAction="#verfacturas" prefHeight="27.0" prefWidth="152.0" text="Ver todas las facturas">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <TableView fx:id="tblCliente" layoutX="93.0" layoutY="251.0" prefHeight="388.0" prefWidth="730.0">
        <columns>
          <TableColumn fx:id="tfCodigo" prefWidth="72.0" text="Numero" />
          <TableColumn fx:id="tfNombre" prefWidth="473.0" text="Cliente" />
            <TableColumn fx:id="tfapellido" prefWidth="184.0" text="Total de Factura" />
        </columns>
      </TableView>
   </children>
</Pane>
